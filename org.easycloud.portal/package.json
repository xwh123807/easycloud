    "font-awesome": "^4.7.0",
    "ionicons": "^3.0.0",
    "jquery": "^3.2.1",
    "js-yaml": "^3.8.4",
    "ng2-charts": "^1.5.0",
    "ng2-echarts": "0.0.3",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
