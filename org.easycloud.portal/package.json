{
  "name": "org.easycloud.portal",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ec=true --proxy-config proxy.conf.json",
    "build": "ng build -prod -aot",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "serve": "http-server --cross -p 4200 --proxy http://localhost:8001 dist",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "jit": "webpack-dev-server --config webpack.jit.config.js",
    "aot": "webpack-dev-server --config webpack.aot.config.js",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "http-server  --cross -p 4200 aot",
    "build:webpack": "webpack",
    "start:webpack": "webpack-dev-server --port=4200",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
